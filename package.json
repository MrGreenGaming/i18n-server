{
  "name": "i18n-server",
  "version": "0.0.1",
  "description": "i18n server for Mr. Green Gaming",
  "author": "AfuSensi",
  "scripts": {
    "build": "node build.js",
    "lint": "tslint --project \"tsconfig.json\"",
    "trylint": "eslint \"*/**/*.{js,ts,tsx}\" --quiet --fix",
    "start": "node -r module-alias/register ./dist --env=production",
    "start:dev": "nodemon"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [],
    "exec": "ts-node -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {},
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@overnightjs/core": "^1.7.4",
    "@overnightjs/logger": "^1.1.12",
    "@types/fs-extra": "^9.0.1",
    "body-parser": "^1.19.0",
    "command-line-args": "^5.1.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-joi-validation": "^4.0.3",
    "helmet": "^3.22.0",
    "http-status-codes": "^1.4.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.1",
    "simple-git": "^2.11.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.6",
    "@types/find": "^0.2.1",
    "@types/hapi__joi": "^17.1.2",
    "@types/helmet": "0.0.47",
    "@types/jasmine": "^3.5.10",
    "@types/jsonfile": "^6.0.0",
    "@types/morgan": "^1.9.0",
    "@types/node": "^14.0.11",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "find": "^0.3.0",
    "fs-extra": "^9.0.1",
    "jasmine": "^3.5.0",
    "jsonfile": "^6.0.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.5"
  }
}
