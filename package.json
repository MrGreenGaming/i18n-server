{
  "name": "i18n-server",
  "version": "0.0.1",
  "description": "i18n server for Mr. Green Gaming",
  "repository": "https://github.com/mrGreenGaming/i18n-server",
  "author": "AfuSensi",
  "scripts": {
    "start:prod": "docker-compose -f docker-compose.prod.yml up -d --build",
    "start:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "stop:prod": "docker-compose -f docker-compose.prod.yml down -v",
    "stop:dev": "docker-compose -f docker-compose.dev.yml down -v",
    "predebug": "rimraf dist/*",
    "debug": "tsc-watch --onSuccess \"node --inspect=0.0.0.0:9229 dist/index.js\" --onFailure \"echo Server compilation failed\"",
    "prod": "node -r module-alias/register ./dist --env=production",
    "build": "node build.js"
  },
  "_moduleAliases": {},
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@overnightjs/core": "^1.7.4",
    "@overnightjs/logger": "^1.1.12",
    "@types/fs-extra": "^9.0.1",
    "@types/gettext-parser": "^4.0.0",
    "@types/shelljs": "^0.8.8",
    "@types/unzipper": "^0.10.3",
    "@types/xml2json": "^0.11.3",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "command-line-args": "^5.1.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-joi-validation": "^4.0.3",
    "gettext-parser": "^4.0.4",
    "helmet": "^3.22.0",
    "http-status-codes": "^1.4.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.1",
    "shelljs": "^0.8.4",
    "simple-git": "^2.20.1",
    "tslib": "^2.0.1",
    "unzipper": "^0.10.11",
    "winston": "^3.2.1",
    "xml2json": "^0.12.0"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.6",
    "@types/find": "^0.2.1",
    "@types/hapi__joi": "^17.1.4",
    "@types/helmet": "0.0.47",
    "@types/jasmine": "^3.5.13",
    "@types/jsonfile": "^6.0.0",
    "@types/morgan": "^1.9.0",
    "@types/node": "^14.6.0",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "find": "^0.3.0",
    "fs-extra": "^9.0.1",
    "jasmine": "^3.6.1",
    "jsonfile": "^6.0.1",
    "prettier": "^2.1.0",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.2",
    "tsc-watch": "^4.2.9",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7"
  }
}
